[cbm]
bufferpool_size=50G
bufferpool_temp_space=0.5
compression_lz4_enabled=true

[memory]
max_memory_size=120G

[engine]
enable_aggregation_disk_spilling=true
enable_hashjoin_disk_spilling=true
max_parallelism_level=180
max_update_parallelism_level=-1

[system]
update_propagation=false
num_cores=20


